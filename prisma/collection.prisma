model Collection {
  id          String   @id @default(uuid())
  name        String
  songs       Song[]   @relation("CollectionToSong")
  isPublic    Boolean  @default(false)
  createdAt   DateTime @default(now()) @map("created_at")
  description String?
  coverUrl    String?  @map("cover_url")

  @@map("collections")
}